{
  "name": "electron-updater-example",
  "version": "1.1.1",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "Matt Haggard",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build",
    "publish": "build -p always"
  },
  "devDependencies": {
    "electron": "9.4.4",
    "electron-builder": "^20.0.0"
  },
  "dependencies": {
    "electron-log": "^1.3.0",
    "electron-updater": "^4.0.6"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://korbsstudio.com/update/"
      }
    ],
    "appId": "com.github.iffy.electronupdaterexample",
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
